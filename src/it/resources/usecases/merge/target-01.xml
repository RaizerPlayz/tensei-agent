<?xml version="1.0" encoding="UTF-8"?>
<dfasdl xmlns="http://www.dfasdl.org/DFASDL"
        default-encoding="utf-8" semantic="niem">
  <str id="firstLine" />
  <seq id="product_list">
    <elem id="product">
      <str id="link" stop-sign="\t"/>
      <str id="title" stop-sign="\t"/>
      <str id="beschreibung" stop-sign="\t"/>
      <str id="bildlink" stop-sign="\t"/>
      <str id="marke" stop-sign="\t"/>
      <formatnum id="preis" format="(-?\d+(\.\d+)?)" decimal-separator="." stop-sign="\t"/>
      <str id="id" stop-sign="\t"/>
      <str id="ean" stop-sign="\t"/>
      <formatnum id="gewicht" format="(-?\d+(\.\d+)?)" decimal-separator="." stop-sign="\t"/>
      <str id="google_cat" stop-sign="\t"/>
      <str id="verfuegbarkeit" stop-sign="\t"/>
      <str id="produkttyp" stop-sign="\t"/>
      <str id="zustand" stop-sign="\t"/>
      <formatnum id="versand" format="(-?\d+(\.\d+)?)" decimal-separator="."/>
    </elem>
  </seq>
</dfasdl>