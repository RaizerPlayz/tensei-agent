<?xml version="1.0" encoding="UTF-8"?>
<dfasdl xmlns="http://www.dfasdl.org/DFASDL"
        default-encoding="utf-8" semantic="niem">
  <seq id="accounts">
    <elem id="row">
      <num id="id" stop-sign=","/>
      <str id="name" max-length="254" stop-sign=","/>
      <str id="description" stop-sign=","/>
      <date id="birthday" stop-sign=","/>
      <formatnum id="salary" format="([-]?\d+[\.?\d*?]*?\,\d{2})"/>
    </elem>
  </seq>
</dfasdl>