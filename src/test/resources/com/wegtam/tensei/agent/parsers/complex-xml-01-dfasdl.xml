        <str id="ku_telefon"/>
        <str id="ku_fax"/>
        <str id="ku_email"/>
        <str id="ku_handy"/>
        <str id="ku_firma"/>
        <num id="k_id"/>
      </elem>
    </seq>
  </seq>
</dfasdl>